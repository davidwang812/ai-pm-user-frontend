{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build:no-check && bash scripts/ensure-dist.sh",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://aiproductmanager-production.up.railway.app",
    "VITE_WS_URL": "wss://aiproductmanager-production.up.railway.app",
    "NODE_VERSION": "18"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://aiproductmanager-production.up.railway.app",
      "VITE_WS_URL": "wss://aiproductmanager-production.up.railway.app",
      "NODE_VERSION": "18"
    }
  }
}