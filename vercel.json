{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://aiproductmanager-production.up.railway.app",
    "VITE_WS_URL": "wss://aiproductmanager-production.up.railway.app",
    "NODE_VERSION": "18"
  }
}