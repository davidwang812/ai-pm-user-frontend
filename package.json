{
  "name": "ai-pm-user",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:local": "npm run prebuild && npx vite build",
    "build:no-check": "vite build",
    "build:vercel": "node node_modules/vite/bin/vite.js build",
    "preview": "vite preview",
    "prebuild": "npm run check:integrity && npm run check:build",
    "check:integrity": "bash scripts/check-project-integrity.sh",
    "check:build": "bash scripts/pre-build-check.sh",
    "build:safe": "npm run prebuild && npm run build:local",
    "build:analyze": "vite build --mode analyze",
    "test": "playwright test",
    "test:smoke": "node tests/e2e/utils/test-runner.js smoke",
    "test:full": "node tests/e2e/utils/test-runner.js full",
    "test:debug": "node tests/e2e/utils/test-runner.js debug",
    "test:report": "playwright show-report",
    "test:ui": "playwright test --ui",
    "test:install": "playwright install",
    "test:auth": "./run-tests.sh -s auth",
    "test:ai": "./run-tests.sh -s ai",
    "test:product": "./run-tests.sh -s product",
    "test:websocket": "playwright test tests/e2e/specs/websocket.spec.js",
    "test:performance": "playwright test tests/e2e/specs/performance.spec.js",
    "test:collect": "node tests/e2e/utils/result-collector.js",
    "test:dashboard": "open tests/dashboard/index.html",
    "test:ci": "playwright test --reporter=json,html"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "dompurify": "^3.0.6",
    "echarts": "^5.4.3",
    "element-plus": "^2.4.3",
    "lodash-es": "^4.17.21",
    "marked": "^11.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "chalk": "^5.3.0",
    "js-yaml": "^4.1.0",
    "playwright": "^1.54.2",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0"
  }
}
